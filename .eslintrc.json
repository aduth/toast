{
	"root": true,
	"parser": "babel-eslint",
	"env": {
		"node": true
	},
	"rules": {
		"array-bracket-spacing": [ 2, "always" ],
		"brace-style": [ 2, "1tbs" ],
		"camelcase": 2,
		"comma-spacing": 2,
		"comma-style": 2,
		"computed-property-spacing": [ 2, "always" ],
		"constructor-super": 2,
		"curly": 2,
		"dot-notation": 2,
		"eqeqeq": [ 2 ],
		"eol-last": 2,
		"indent": [ 2, "tab", { "SwitchCase": 1 } ],
		"jsx-quotes": [ 2, "prefer-double" ],
		"key-spacing": 2,
		"keyword-spacing": 2,
		"no-cond-assign": 2,
		"no-const-assign": 2,
		"no-debugger": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-else-return": 2,
		"no-empty": [ 2, { "allowEmptyCatch": true } ],
		"no-extra-semi": 2,
		"no-lonely-if": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": [ 2, { "max": 1 } ],
		"no-multi-spaces": 2,
		"no-negated-in-lhs": 2,
		"no-nested-ternary": 2,
		"no-new": 2,
		"no-process-exit": 2,
		"no-redeclare": 2,
		"no-shadow": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"no-undef": 2,
		"no-unreachable": 2,
		"no-unused-vars": 2,
		"no-use-before-define": [ 2, "nofunc" ],
		"object-curly-spacing": [ 2, "always" ],
		"operator-linebreak": [ 1, "after", { "overrides": {
			"?": "before",
			":": "before"
		} } ],
		"padded-blocks": [ 2, "never" ],
		"prefer-const": 2,
		"quote-props": [ 2, "as-needed", { "keywords": true } ],
		"quotes": [ 2, "single", "avoid-escape" ],
		"semi": 2,
		"semi-spacing": 2,
		"space-before-blocks": [ 2, "always" ],
		"space-before-function-paren": [ 2, "never" ],
		"space-in-parens": [ 2, "always" ],
		"space-infix-ops": [ 2, { "int32Hint": false } ],
		"space-unary-ops": [ 2, {
			"overrides": {
				"!": true
			}
		} ],
		"strict": [ 2, "never" ],
		"valid-jsdoc": [ 2, { "requireReturn": false } ],
		"vars-on-top": 2,
		"yoda": [ 2, "always", { "onlyEquality": true } ]
	}
}
